<html>
    <head>
    </head>
    <body>
        <p class="bg-primary">GENERATE SCHEDULER</p>
        <div class="page-header">
        <h3>GENERATE</h3>
        </div>
        <% if flash[:warning] %>
            <label class="invalid-input"><%= flash[:warning] %></label>
        <% end %>
        <%= form_tag("manage_room", method: "get",class: 'form-inline') do %>
            <%= label_tag(:q, "Start date:") %>
            <%= select_date Date.today, prefix: :start_date %>
            <%= label_tag(:s, "End date:") %>
            <%= select_date Date.today, prefix: :end_date %>
            <%= submit_tag "Generate!!",class: 'btn btn-danger' %>
        <% end %>
        
    </body>
</html>